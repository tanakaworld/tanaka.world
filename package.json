{
  "name": "tanaka.world",
  "version": "1.0.0",
  "description": "The TANAKA WORLD",
  "author": "tanakaworld",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt-ts",
    "build": "NODE_ENV=production nuxt-ts build",
    "start": "NODE_ENV=production nuxt-ts start",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "lint": "eslint --ext .vue,.ts .",
    "lintfix": "npm run lint -- --fix",
    "test": "jest",
    "precommit": "npm run lint",
    "serve": "firebase serve --only hosting",
    "deploy": "firebase use tanaka-world && firebase deploy --only hosting"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.3.5",
    "@nuxtjs/google-analytics": "2.2.2",
    "color-convert": "2.0.1",
    "cross-env": "6.0.3",
    "normalize.css": "^8.0.1",
    "nuxt": "2.11.0",
    "nuxt-i18n": "^6.4.1",
    "randomcolor": "0.5.4",
    "typescript": "3.7.4",
    "vue-i18n": "^8.15.3",
    "vuex-typescript-fsa": "0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^7.5.0",
    "@nuxt/typescript-build": "^0.5.4",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.7.4",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^2.6.0",
    "jest": "^24.9.0",
    "jest-date-mock": "^1.0.7",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.2.0",
    "vue-jest": "^3.0.5"
  }
}
