{
  "name": "tanaka.world",
  "version": "1.0.0",
  "description": "The TANAKA WORLD",
  "author": "tanakaworld",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt-ts",
    "build": "NODE_ENV=production nuxt-ts build",
    "start": "NODE_ENV=production nuxt-ts start",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "precommit": "npm run lint",
    "serve": "firebase serve --only hosting",
    "deploy": "firebase use tanaka-world && firebase deploy --only hosting"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "color-convert": "2.0.1",
    "cross-env": "7.0.3",
    "normalize.css": "8.0.1",
    "typescript": "3.7.4",
    "nuxt": "2.14.12",
    "nuxt-i18n": "6.16.0",
    "randomcolor": "0.6.2",
    "vue-i18n": "8.22.2",
    "vuex-typescript-fsa": "0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/types": "2.14.12",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@types/jest": "26.0.19",
    "@vue/test-utils": "1.1.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.2",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "lint-staged": "10.5.3",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "sass-loader": "10.1.0",
    "ts-jest": "26.4.4",
    "vue-jest": "3.0.7"
  }
}
