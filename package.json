{
  "name": "tanaka.world",
  "version": "1.0.0",
  "description": "The TANAKA WORLD",
  "author": "tanakaworld",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt-ts",
    "build": "NODE_ENV=production nuxt-ts build",
    "start": "NODE_ENV=production nuxt-ts start",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "test": "jest",
    "e2e:local": "start-server-and-test start http://localhost:3000 'cypress open'",
    "e2e:ci": "CYPRESS_video=true start-server-and-test start http://localhost:3000 'cypress run'",
    "e2e:generate": "npm run generate -- --fail-on-error",
    "precommit": "npm run lint",
    "serve": "firebase serve --only hosting",
    "deploy": "firebase use tanaka-world && firebase deploy --only hosting"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "color-convert": "2.0.1",
    "cross-env": "7.0.3",
    "normalize.css": "8.0.1",
    "nuxt": "2.15.3",
    "nuxt-i18n": "6.21.1",
    "randomcolor": "0.6.2",
    "vue-i18n": "8.24.1",
    "vuex-typescript-fsa": "0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@cypress/webpack-preprocessor": "5.6.0",
    "@nuxt/types": "2.15.3",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config": "6.0.0",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@testing-library/cypress": "7.0.4",
    "@types/jest": "26.0.21",
    "@vue/test-utils": "1.1.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "cypress": "6.8.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.7.0",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "node-sass": "5.0.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "sass-loader": "10.1.1",
    "start-server-and-test": "1.12.1",
    "ts-jest": "26.5.4",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.2.3",
    "vue-jest": "3.0.7"
  }
}
