{
  "name": "tanaka.world",
  "version": "1.0.0",
  "description": "The TANAKA WORLD",
  "author": "tanakaworld",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt-ts",
    "build": "NODE_ENV=production nuxt-ts build",
    "start": "NODE_ENV=production nuxt-ts start",
    "generate": "NODE_ENV=production nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "precommit": "npm run lint",
    "serve": "firebase serve --only hosting",
    "deploy": "firebase use tanaka-world && firebase deploy --only hosting"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.6",
    "@nuxtjs/google-analytics": "^2.0.2",
    "color-convert": "^1.9.3",
    "cross-env": "^5.2.0",
    "nuxt": "^2.9.2",
    "randomcolor": "^0.5.3",
    "typescript": "^3.6.2",
    "vuex-typescript-fsa": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@nuxt/typescript-build": "^0.2.9",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/node": "^12.7.4",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "vue-jest": "^4.0.0-0"
  }
}
